# ASP.NET Core Environment
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=https://+:443;http://+:80

# Event Bus Configuration
EventBus__Provider=RabbitMQ

# RabbitMQ Configuration
RABBITMQ_HOST=rabbitmq.production.local
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=cardactions
RABBITMQ_PASSWORD=your-secure-password
RABBITMQ_EXCHANGE=cardactions.events
RABBITMQ_VHOST=/production

# IBM MQ Configuration (use when EventBus__Provider=IbmMQ)
IBMMQ_QUEUE_MANAGER=QM1
IBMMQ_HOST=ibmmq.production.local
IBMMQ_PORT=1414
IBMMQ_CHANNEL=PROD.APP.SVRCONN
IBMMQ_QUEUE_NAME=PROD.CARDACTIONS.EVENTS
IBMMQ_USERNAME=cardactions
IBMMQ_PASSWORD=your-secure-ibmmq-password
IBMMQ_USE_SSL=true
IBMMQ_SSL_CIPHER_SPEC=TLS_RSA_WITH_AES_256_GCM_SHA384

# Logging
Serilog__MinimumLevel__Default=Information
Serilog__MinimumLevel__Override__Microsoft=Warning