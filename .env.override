# ASP.NET Core Environment
ASPNETCORE_ENVIRONMENT=Development

# Event Bus Provider (InMemory | RabbitMQ | IbmMQ)
EventBus__Provider=InMemory

# RabbitMQ Configuration (for development)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=cardactions
RABBITMQ_PASSWORD=devpassword
RABBITMQ_EXCHANGE=cardactions.events
RABBITMQ_VHOST=/

# IBM MQ Configuration (for development)
IBMMQ_QUEUE_MANAGER=QM1
IBMMQ_HOST=localhost
IBMMQ_PORT=1414
IBMMQ_CHANNEL=DEV.APP.SVRCONN
IBMMQ_QUEUE_NAME=DEV.QUEUE.1
IBMMQ_USERNAME=app
IBMMQ_PASSWORD=passw0rd
IBMMQ_USE_SSL=false